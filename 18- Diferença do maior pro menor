v1=int(input("Digite um valor "))
v2=int(input("Digite outro valor "))
if v1 > v2 :
    print ("A diferença do maior número para o menor é " ,v1-v2)
else :
    if v1 < v2 :
        print ("A diferença do maior número para o menor é " ,v2-v1)
    else :
        print ("Os valores são iguais, portanto não possuem diferença ")