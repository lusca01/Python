v1=int(input("Digite um valor "))
v2=int(input("Digite um valor maior que o anterior "))
v3=int(input("Digite um valor maior que o anterior "))
v4=int(input("Digite qualquer valor "))
if v1 > v2 or v2 > v3 or v1 > v3 :
    print ("Os valores informados est√£o incorretos ")
else :
    if v4 < v1 :
        print ("Os valores na ordem crescente ficam: " ,v4, v1, v2, v3)
    else :
        if v4 > v1 and v4 < v2 :
            print ("Os valores na ordem crescente ficam: " ,v1, v4, v2, v3)
        else :
            if v4 > v2 and v4 < v3 :
                print ("Os valores na ordem crescente ficam: " ,v1, v2, v4, v3)
            else :
                print ("Os valores na ordem crescente ficam: " ,v1, v2, v3, v4)